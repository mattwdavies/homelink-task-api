    "express": "^4.21.1",
    "knex": "^3.1.0",
    "pg": "^8.13.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "uuid": "^11.0.3",
    "yup": "^1.5.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
