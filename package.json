  "scripts": {
    "test": "jest",
    "build": "tsc",
    "dev": "nodemon --exec ts-node src/app.ts",
    "migrate": "ts-node -r tsconfig-paths/register ./node_modules/.bin/knex migrate:latest --knexfile src/knexfile.ts",
